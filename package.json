{
  "name": "@folio/workflow",
  "version": "1.0.0",
  "description": "A GUI for building workflows.",
  "main": "src/index.tsx",
  "repository": "",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=12.20.1"
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./output",
    "build-mod-descriptor": "stripes mod descriptor --full --strict | jq '.[]' > module-descriptor.json ",
    "coverage": "jest --coverage --passWithNoTests",
    "formatjs-compile": "formatjs compile-folder --ast --format simple ./translations/ui-workflow ./translations/ui-workflow/compiled",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "test": "jest --passWithNoTests",
    "tsc:check-app": "tsc --noEmit --project \"./tsconfig.json\""
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.9.0",
    "@folio/eslint-config-stripes": "^7.0.0",
    "@folio/stripes": "^9.0.4",
    "@folio/stripes-cli": "^3.0.1",
    "@folio/stripes-components": "^12.0.4",
    "@folio/stripes-smart-components": "^9.0.1",
    "@formatjs/cli": "^4.2.29",
    "@jest/globals": "^26.6.2",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-router-dom": "^5.3.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "chai": "^4.2.0",
    "core-js": "^3.6.4",
    "eslint": "^7.32.0",
    "inflected": "^2.0.4",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.3.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^12.0.0",
    "miragejs": "^0.1.48",
    "mocha": "^5.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.0.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-observable": "^1.2.0",
    "regenerator-runtime": "^0.13.3",
    "rxjs": "^6.6.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@folio/stripes-acq-components": "^5.0.100000791",
    "lodash": "^4.17.21",
    "prop-types": "^15.6.0"
  },
  "peerDependencies": {
    "@folio/stripes": "^9.0.0",
    "react": "^18.2.0",
    "react-intl": "^6.0.0",
    "react-redux": "^8.0.5",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "optionalDependencies": {
    "@folio/plugin-query-builder": ">=1.0.0"
  },
  "stripes": {
    "actsAs": [
      "app",
      "settings"
    ],
    "displayName": "ui-workflow.title",
    "route": "/workflow",
    "home": "/workflow",
    "hasSettings": false,
    "okapiInterfaces": {
      "workflow-actions": "1.0",
      "workflow-events": "1.0",
      "workflow-triggers": "1.0",
      "workflow-tasks": "1.0",
      "workflow-workflows": "1.0"
    },
    "stripesDeps": [
      "@folio/stripes-acq-components"
    ],
    "permissionSets": [
      {
        "permissionName": "module.workflow.enabled",
        "displayName": "UI: ui-workflow module is enabled",
        "visible": false
      },
      {
        "permissionName": "settings.workflow.enabled",
        "displayName": "Settings (workflow): display list of settings pages",
        "subPermissions": [
          "settings.enabled"
        ],
        "visible": false
      },
      {
        "permissionName": "ui-workflow.all",
        "displayName": "Full access to all UI-Workflow functionality.",
        "subPermissions": [
          "workflow.actions.all",
          "workflow.events.all",
          "workflow.triggers.all",
          "workflow.tsks.all",
          "workflow.workflows.all"
        ],
        "visible": true
      }
    ],
    "icons": [
      {
        "name": "app",
        "alt": "Create, view and manage workflows.",
        "title": "Workflow"
      }
    ]
  },
  "resolutions": {
    "@folio/stripes-types": "^2.1.0"
  }
}
