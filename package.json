{
  "name": "@folio/workflow",
  "version": "1.0.0",
  "description": "A GUI for building workflows.",
  "main": "src/index.tsx",
  "repository": "",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./output",
    "build-mod-descriptor": "stripes mod descriptor --full --strict | jq '.[]' > module-descriptor.json ",
    "coverage": "jest --coverage --passWithNoTests",
    "formatjs-compile": "formatjs compile-folder --ast --format simple ./translations/ui-workflow ./translations/ui-workflow/compiled",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "test": "jest --passWithNoTests",
    "tsc:check-app": "tsc --noEmit --project \"./tsconfig.json\""
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@folio/eslint-config-stripes": "^8.0.0",
    "@folio/stripes": "^10.0.0",
    "@folio/stripes-acq-components": "^7.0.0",
    "@folio/stripes-cli": "^4.0.0",
    "@folio/stripes-components": "^13.0.0",
    "@folio/stripes-connect": "^10.0.0",
    "@folio/stripes-core": "^11.0.0",
    "@folio/stripes-data-transfer-components": "^7.0.0",
    "@folio/stripes-final-form": "^9.0.0",
    "@folio/stripes-form": "^10.0.0",
    "@folio/stripes-logger": "^1.0.0",
    "@folio/stripes-smart-components": "^10.0.0",
    "@folio/stripes-types": "^3.0.0",
    "@folio/stripes-util": "^7.0.0",
    "@jest/globals": "^29.5.0",
    "@rehooks/local-storage": "^2.4.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.9.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.17.0",
    "@types/react-router-dom": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@typescript-eslint/typescript-estree": "^6.0.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^29.5.0",
    "eslint": "^7.32.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "final-form": "^4.20.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-junit": "^16.0.0",
    "lodash": "^4.17.0",
    "miragejs": "^0.1.40",
    "moment": "^2.29.0",
    "popper.js": "^1.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-intl": "^7.1.5",
    "react-redux": "^8.0.5",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.0",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.6.7",
    "ts-jest": "^29.4.0"
  },
  "dependencies": {},
  "peerDependencies": {
    "@folio/stripes": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-intl": "^7.1.5",
    "react-redux": "^8.0.5",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "optionalDependencies": {},
  "stripes": {
    "actsAs": [
      "app",
      "settings"
    ],
    "displayName": "ui-workflow.title",
    "route": "/workflow",
    "home": "/workflow",
    "hasSettings": false,
    "okapiInterfaces": {
      "workflow-actions": "1.0",
      "workflow-events": "1.0",
      "workflow-nodes": "1.0",
      "workflow-triggers": "1.0",
      "workflow-tasks": "1.0",
      "workflow-workflows": "1.0"
    },
    "stripesDeps": [
      "@folio/stripes-acq-components"
    ],
    "permissionSets": [
      {
        "permissionName": "module.workflow.enabled",
        "displayName": "UI: ui-workflow module is enabled",
        "visible": true
      },
      {
        "permissionName": "settings.workflow.enabled",
        "displayName": "Settings (workflow): Display list of settings pages",
        "subPermissions": [
          "settings.enabled"
        ],
        "visible": true
      },
      {
        "permissionName": "settings.workflow.configure",
        "displayName": "Settings (workflow): Make changes to the workflow settings",
        "subPermissions": [
          "settings.enabled"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-workflow.all",
        "displayName": "Workflow (Admin): All permissions",
        "subPermissions": [
          "workflow.actions.all",
          "workflow.events.all",
          "workflow.nodes.all",
          "workflow.tasks.all",
          "workflow.triggers.all",
          "workflow.workflows.all"
        ],
        "visible": true
      }
    ],
    "icons": [
      {
        "name": "app",
        "alt": "Create, view and manage workflows.",
        "title": "Workflow"
      }
    ]
  },
  "resolutions": {}
}
